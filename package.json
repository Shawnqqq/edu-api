{
  "name": "jiker-neon",
  "version": "1.0.0",
  "description": "nodejs eggjs simple",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.1",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "factory-girl": "^5.0.4",
    "husky": "^4.2.3",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-jiker-neon",
    "start:local": "EGG_SERVER_ENV=local egg-scripts start --daemon --title=egg-server-jiker-neon",
    "stop": "egg-scripts stop --title=egg-server-jiker-neon",
    "dev": "EGG_SERVER_ENV=local egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && NODE_ENV=test npx sequelize db:migrate && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Jax Chu",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
